<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph.xml"
    app:startDestination="@id/cv_host_fragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.example.mh.face_detection2.FirstFragment"
        tools:layout="@layout/first_fragment">
        <action
            android:id="@+id/action_FirstFragment"
            app:destination="@id/SecondFragment"/>
    </fragment>
    <fragment
        android:id="@+id/SecondFragment"
        android:name="com.example.mh.face_detection2.SecondFragment"
        tools:layout="@layout/second_fragment">
        <action
            android:id="@+id/action_SecondFragment"
            app:destination="@id/FirstFragment"/>
    </fragment>
</navigation>